{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "description": "Four Golden Signals dashboard for services. Supports HTTP (via Hubble eBPF) and gRPC (via application metrics). Shows Latency, Traffic, Errors, and Saturation.",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 100,
      "panels": [],
      "title": "Overview - $namespace / $workload",
      "type": "row"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.01 },
              { "color": "red", "value": 0.05 }
            ]
          },
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "(\n  sum(rate(hubble_http_requests_total{destination_namespace=~\"$namespace\", destination_workload=~\"$workload\", status=~\"5..\"}[$__rate_interval]))\n  + sum(rate(grpc_server_handled_total{grpc_code!=\"OK\", namespace=~\"$namespace\", pod=~\"$workload.*\"}[$__rate_interval]) or vector(0))\n  + sum(rate(${workload}_grpc_requests_total{status!=\"OK\", namespace=~\"$namespace\"}[$__rate_interval]) or vector(0))\n) / (\n  sum(rate(hubble_http_requests_total{destination_namespace=~\"$namespace\", destination_workload=~\"$workload\"}[$__rate_interval]))\n  + sum(rate(grpc_server_handled_total{namespace=~\"$namespace\", pod=~\"$workload.*\"}[$__rate_interval]) or vector(0))\n  + sum(rate(${workload}_grpc_requests_total{namespace=~\"$namespace\"}[$__rate_interval]) or vector(0))\n) or vector(0)",
          "legendFormat": "Error Rate",
          "refId": "A"
        }
      ],
      "title": "Error Rate (HTTP+gRPC)",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 500 }
            ]
          },
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "id": 2,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "max(\n  histogram_quantile(0.95, sum(rate(hubble_http_request_duration_seconds_bucket{destination_namespace=~\"$namespace\", destination_workload=~\"$workload\"}[$__rate_interval])) by (le)) * 1000,\n  histogram_quantile(0.95, sum(rate(grpc_server_handling_seconds_bucket{namespace=~\"$namespace\", pod=~\"$workload.*\"}[$__rate_interval])) by (le)) * 1000,\n  histogram_quantile(0.95, sum(rate(${workload}_grpc_request_duration_seconds_bucket{namespace=~\"$namespace\"}[$__rate_interval])) by (le)) * 1000\n) or histogram_quantile(0.95, sum(rate(hubble_http_request_duration_seconds_bucket{destination_namespace=~\"$namespace\", destination_workload=~\"$workload\"}[$__rate_interval])) by (le)) * 1000",
          "legendFormat": "P95 Latency",
          "refId": "A"
        }
      ],
      "title": "P95 Latency",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          },
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "id": 3,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(rate(hubble_http_requests_total{destination_namespace=~\"$namespace\", destination_workload=~\"$workload\"}[$__rate_interval])) + sum(rate(grpc_server_started_total{namespace=~\"$namespace\", pod=~\"$workload.*\"}[$__rate_interval]) or vector(0)) + sum(rate(${workload}_grpc_requests_total{namespace=~\"$namespace\"}[$__rate_interval]) or vector(0))",
          "legendFormat": "RPS",
          "refId": "A"
        }
      ],
      "title": "Traffic (RPS)",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "id": 4,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "avg(sum by (pod) (container_memory_working_set_bytes{namespace=~\"$namespace\", pod=~\"$workload.*\"}) / sum by (pod) (kube_pod_container_resource_limits{namespace=~\"$namespace\", pod=~\"$workload.*\", resource=\"memory\"}) * 100)",
          "legendFormat": "Memory %",
          "refId": "A"
        }
      ],
      "title": "Saturation (Memory)",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 101,
      "panels": [],
      "title": "HTTP Metrics (Hubble eBPF)",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
          "unit": "ms"
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "P50" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "P95" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "P99" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "id": 10,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "histogram_quantile(0.50, sum(rate(hubble_http_request_duration_seconds_bucket{destination_namespace=~\"$namespace\", destination_workload=~\"$workload\"}[$__rate_interval])) by (le)) * 1000", "legendFormat": "P50", "refId": "A" },
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "histogram_quantile(0.95, sum(rate(hubble_http_request_duration_seconds_bucket{destination_namespace=~\"$namespace\", destination_workload=~\"$workload\"}[$__rate_interval])) by (le)) * 1000", "legendFormat": "P95", "refId": "B" },
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "histogram_quantile(0.99, sum(rate(hubble_http_request_duration_seconds_bucket{destination_namespace=~\"$namespace\", destination_workload=~\"$workload\"}[$__rate_interval])) by (le)) * 1000", "legendFormat": "P99", "refId": "C" }
      ],
      "title": "HTTP Latency (P50/P95/P99)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false, "axisColorMode": "text", "axisPlacement": "auto",
            "drawStyle": "line", "fillOpacity": 30, "lineWidth": 1, "showPoints": "never",
            "stacking": { "group": "A", "mode": "normal" }
          },
          "unit": "reqps"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "id": 20,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(hubble_http_requests_total{destination_namespace=~\"$namespace\", destination_workload=~\"$workload\"}[$__rate_interval])) by (status)", "legendFormat": "{{status}}", "refId": "A" }
      ],
      "title": "HTTP Request Rate by Status Code",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "id": 200,
      "panels": [],
      "title": "gRPC Metrics (Application)",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "gRPC latency from go-grpc-prometheus interceptor or custom metrics.\nSupports: grpc_server_handling_seconds (go-grpc-prometheus) and {service}_grpc_request_duration_seconds (custom)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false, "axisColorMode": "text", "axisPlacement": "auto",
            "drawStyle": "line", "fillOpacity": 10, "lineWidth": 1, "showPoints": "never",
            "stacking": { "group": "A", "mode": "none" }
          },
          "unit": "ms"
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "P50" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "P95" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "P99" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "id": 210,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "histogram_quantile(0.50, sum(rate(grpc_server_handling_seconds_bucket{namespace=~\"$namespace\", pod=~\"$workload.*\"}[$__rate_interval])) by (le)) * 1000 or histogram_quantile(0.50, sum(rate(health_demo_grpc_request_duration_seconds_bucket{namespace=~\"$namespace\"}[$__rate_interval])) by (le)) * 1000", "legendFormat": "P50", "refId": "A" },
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "histogram_quantile(0.95, sum(rate(grpc_server_handling_seconds_bucket{namespace=~\"$namespace\", pod=~\"$workload.*\"}[$__rate_interval])) by (le)) * 1000 or histogram_quantile(0.95, sum(rate(health_demo_grpc_request_duration_seconds_bucket{namespace=~\"$namespace\"}[$__rate_interval])) by (le)) * 1000", "legendFormat": "P95", "refId": "B" },
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "histogram_quantile(0.99, sum(rate(grpc_server_handling_seconds_bucket{namespace=~\"$namespace\", pod=~\"$workload.*\"}[$__rate_interval])) by (le)) * 1000 or histogram_quantile(0.99, sum(rate(health_demo_grpc_request_duration_seconds_bucket{namespace=~\"$namespace\"}[$__rate_interval])) by (le)) * 1000", "legendFormat": "P99", "refId": "C" }
      ],
      "title": "gRPC Latency (P50/P95/P99)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "gRPC request rate by method and status code.\nSupports: grpc_server_handled_total (go-grpc-prometheus) and {service}_grpc_requests_total (custom)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false, "axisColorMode": "text", "axisPlacement": "auto",
            "drawStyle": "line", "fillOpacity": 30, "lineWidth": 1, "showPoints": "never",
            "stacking": { "group": "A", "mode": "normal" }
          },
          "unit": "reqps"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "id": 220,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(grpc_server_handled_total{namespace=~\"$namespace\", pod=~\"$workload.*\"}[$__rate_interval])) by (grpc_method, grpc_code) or sum(rate(health_demo_grpc_requests_total{namespace=~\"$namespace\"}[$__rate_interval])) by (method, status)", "legendFormat": "{{grpc_method}}:{{grpc_code}}{{method}}:{{status}}", "refId": "A" }
      ],
      "title": "gRPC Request Rate by Method/Status",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "gRPC error rate (non-OK responses)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false, "axisColorMode": "text", "axisPlacement": "auto",
            "drawStyle": "line", "fillOpacity": 10, "lineWidth": 1, "showPoints": "never"
          },
          "unit": "percentunit"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 23 },
      "id": 230,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(grpc_server_handled_total{namespace=~\"$namespace\", pod=~\"$workload.*\", grpc_code!=\"OK\"}[$__rate_interval])) / sum(rate(grpc_server_handled_total{namespace=~\"$namespace\", pod=~\"$workload.*\"}[$__rate_interval])) or sum(rate(health_demo_grpc_requests_total{namespace=~\"$namespace\", status!=\"OK\"}[$__rate_interval])) / sum(rate(health_demo_grpc_requests_total{namespace=~\"$namespace\"}[$__rate_interval]))", "legendFormat": "gRPC Error Rate", "refId": "A" }
      ],
      "title": "gRPC Error Rate",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Active gRPC streams (for streaming RPCs)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false, "axisColorMode": "text", "axisPlacement": "auto",
            "drawStyle": "line", "fillOpacity": 30, "lineWidth": 1, "showPoints": "never"
          },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 23 },
      "id": 240,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(grpc_server_started_total{namespace=~\"$namespace\", pod=~\"$workload.*\"}) - sum(grpc_server_handled_total{namespace=~\"$namespace\", pod=~\"$workload.*\"})", "legendFormat": "Active Streams", "refId": "A" }
      ],
      "title": "Active gRPC Streams",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 31 },
      "id": 103,
      "panels": [],
      "title": "Errors",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false, "axisColorMode": "text", "axisPlacement": "auto",
            "drawStyle": "line", "fillOpacity": 30, "lineWidth": 1, "showPoints": "never"
          },
          "unit": "percentunit"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 32 },
      "id": 30,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(hubble_http_requests_total{destination_namespace=~\"$namespace\", destination_workload=~\"$workload\", status=~\"5..\"}[$__rate_interval])) / sum(rate(hubble_http_requests_total{destination_namespace=~\"$namespace\", destination_workload=~\"$workload\"}[$__rate_interval]))", "legendFormat": "HTTP 5xx", "refId": "A" },
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(hubble_http_requests_total{destination_namespace=~\"$namespace\", destination_workload=~\"$workload\", status=~\"4..\"}[$__rate_interval])) / sum(rate(hubble_http_requests_total{destination_namespace=~\"$namespace\", destination_workload=~\"$workload\"}[$__rate_interval]))", "legendFormat": "HTTP 4xx", "refId": "B" }
      ],
      "title": "HTTP Error Rate (4xx / 5xx)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false, "axisColorMode": "text", "axisPlacement": "auto",
            "drawStyle": "line", "fillOpacity": 30, "lineWidth": 1, "showPoints": "never",
            "stacking": { "group": "A", "mode": "normal" }
          },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 32 },
      "id": 31,
      "options": {
        "legend": { "calcs": ["sum"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(increase(hubble_drop_total{destination_namespace=~\"$namespace\"}[$__rate_interval])) by (reason)", "legendFormat": "{{reason}}", "refId": "A" }
      ],
      "title": "Network Drops by Reason (Hubble)",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 40 },
      "id": 104,
      "panels": [],
      "title": "Saturation",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false, "axisColorMode": "text", "axisPlacement": "auto",
            "drawStyle": "line", "fillOpacity": 30, "lineWidth": 1, "showPoints": "never"
          },
          "unit": "percent"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 41 },
      "id": 40,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(container_cpu_usage_seconds_total{namespace=~\"$namespace\", pod=~\"$workload.*\"}[$__rate_interval])) by (pod) * 100", "legendFormat": "{{pod}}", "refId": "A" }
      ],
      "title": "CPU Usage by Pod",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
